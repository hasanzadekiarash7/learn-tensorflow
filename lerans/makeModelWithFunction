      "cell_type": "code",
      "source": [
        "# ورودی اول: تصویر 28x28x1\n",
        "input_image = Input(shape=(28, 28, 1), name='image_input')\n",
        "x1 = Flatten()(input_image)\n",
        "x1 = Dense(64, activation='relu')(x1)\n",
        "\n",
        "# ورودی دوم: داده عددی 10 بعدی\n",
        "input_numeric = Input(shape=(10,), name='numeric_input')\n",
        "x2 = Dense(32, activation='relu')(input_numeric)\n",
        "\n",
        "# ترکیب دو شاخه\n",
        "combined = Concatenate()([x1, x2])\n",
        "x = Dense(64, activation='relu')(combined)\n",
        "\n",
        "# خروجی اول: دسته‌بندی 10 کلاسه\n",
        "output_class = Dense(10, activation='softmax', name='class_output')(x)\n",
        "\n",
        "# خروجی دوم: رگرسیون (یک مقدار عددی)\n",
        "output_regression = Dense(1, name='regression_output')(x)\n",
        "\n",
        "# ساخت مدل با ورودی‌ها و خروجی‌ها\n",
        "model = Model(inputs=[input_image, input_numeric], outputs=[output_class, output_regression])\n",
        "\n",
        "# نمایش خلاصه مدل\n",
        "model.summary()"
      ],
